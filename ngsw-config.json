{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "Sirio",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.woff2",
          "/*.woff",
          "/*.otf",
          "/*.ttf",
          "/*.svg",
          "/*.eot",
          "/*.png",
          "/*.jpg",
          "/*.jpeg",
          "/*.gif"
        ]
      }
    }
  ]
  
}
